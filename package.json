{
  "name": "@ather-mcp/gaia-mcp-server",
  "version": "1.3.1",
  "description": "MCP Server of ProtoGaia",
  "main": "index.js",
  "type": "module",
  "bin": {
    "gaia-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "cz",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "start:sse": "node dist/index.js sse",
    "start:stdio": "node dist/index.js stdio",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "dev:sse": "tsc-watch --onSuccess \"node dist/index.js sse\"",
    "dev:stdio": "tsc-watch --onSuccess \"node dist/index.js stdio\"",
    "dev:nodemon": "nodemon",
    "dev:nodemon:sse": "nodemon --exec \"node dist/index.js sse\"",
    "dev:nodemon:stdio": "nodemon --exec \"node dist/index.js stdio\"",
    "test:logger": "node dist/test-logger.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "check": "npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.782.0",
    "@aws-sdk/credential-providers": "^3.782.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "axios": "^1.8.4",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "image-size": "^2.0.2",
    "ioredis": "^5.6.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^10.3.1",
    "sharp": "^0.34.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.0",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
